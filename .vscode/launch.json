{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Streamlit App",
        "type": "debugpy",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app:app",
          "--reload"
        ],
        "cwd": "${workspaceFolder}/src",
        "python": "${workspaceFolder}/venv/Scripts/python.exe",
        "console": "integratedTerminal"
      }
    ]
  }